{
	"name": "__MSG_extname__",
	"description": "__MSG_extdesc__",
	"version": "1.0.0",
	"manifest_version": 3,
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"action": {
		"default_title": "__MSG_tooltip__",
